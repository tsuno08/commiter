{
  "name": "commiter",
  "version": "1.0.0",
  "description": "",
  "main": "./src/extension.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@vscode/vsce": "^3.3.2",
    "typescript": "^5.8.3"
  },
  "contributes": {
    "commands": [
      {
        "command": "commiter.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "commiter.generateCommitMessage",
